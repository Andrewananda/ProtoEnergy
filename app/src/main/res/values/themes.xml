<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProtoEnergy" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorAccent</item>
        <item name="colorOnPrimary">@color/colorPrimary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorPrimary</item>
        <item name="colorSecondaryVariant">@color/colorAccent</item>
        <item name="colorOnSecondary">@color/colorPrimary</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="actionOverflowButtonStyle">@style/MyHumbergerIcon</item>
        <!-- Customize your theme here. -->
    </style>



    <style name="floating_btn" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="iconTint">@color/colorPrimary</item>
        <item name="backgroundTint">@color/white</item>
    </style>


    <style name="MyHumbergerIcon" parent="android:Widget.Holo.ActionButton.Overflow">
        <item name="android:src">@drawable/filter_list</item>
    </style>
</resources>